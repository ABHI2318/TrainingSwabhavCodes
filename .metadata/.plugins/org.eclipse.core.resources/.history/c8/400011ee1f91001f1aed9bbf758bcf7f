package com.aurion.model.Factory.Test;
import com.aurion.model.Factory.model.CarFactory;
import com.aurion.model.Factory.model.CarType;
import com.aurion.model.Factory.model.ICar;
import com.aurion.model.Factory.model.mahindra;
import com.aurion.model.Factory.model.marauti;
import com.aurion.model.Factory.model.tata;
public class test {

	public static void main(String[] args) {
	ICar car;
	car=CarFactory.makeCar(CarType.mahindra);
	car.start();
	car.stop();
	car=CarFactory.makeCar(CarType.maruti);
	car.start();
	car.stop();
	car=CarFactory.makeCar(CarType.tata);
	car.start();
	car.stop();
	
	
	}

}
