import com.aurion.model.Dog;

public class AnimalTest {

	public static void main(String[] args) {
		Dog dog=new Dog();
		dog.sleep();
		Cat cat=new Cat();
		cat.eat();
		System.out.println(Animal.Max_age);//casn access via animal as well
		System.out.println(Dog.Max_age);//can acess via 
		Animal.info();//only interface can access directly that method
		

	}

}
