package com.aurion.isp.solution.model;

public class Robot implements IRobot{

	@Override
	public void charge() {
		System.out.println("robot us charging");
		
	}

	@Override
	public void startwork() {
		System.out.println("robot is starting to work");
		
	}

	@Override
	public void stopwork() {
	System.out.println("robot is stopping the work");
		
	}
	
	

}
