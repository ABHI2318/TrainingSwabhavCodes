package com.aurion.AbstractBank.model;

public class SavingAccount implements IAccount {
     private int id;
     private String name;
	
	

	@Override
	public String toString() {
		return "SavingAccount [id=" + id + ", name=" + name + "]";
	}

	public SavingAccount(int id, String name) {
		super();
		this.id = id;
		this.name = name;
	}

	@Override
	public void credit(double amount) {
		System.out.println("the money credit in credit account is "+amount);
		
	}

	@Override
	public void debit(double amount) {
		System.out.println("the money debit in savinf account is "+amount);
		
	}

}
